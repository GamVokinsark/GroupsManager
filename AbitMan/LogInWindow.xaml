<Window x:Name="LogInWind" x:Class="AbitMan.LogInWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AbitMan"
        mc:Ignorable="d" Height="194.399" Width="272.938" ResizeMode="NoResize" Title="GroupsManager: Окно входа" WindowStartupLocation="CenterScreen" Closed="LogInWind_Closed">
    <Grid>
        <Button x:Name="LogInButton" Content="Войти" HorizontalAlignment="Left" Margin="89,105,0,0" VerticalAlignment="Top" Width="75" IsDefault="True" FontSize="14" Click="LogInButton_Click"/>
        <Label Content="Логин:" HorizontalAlignment="Left" Margin="17,20,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label Content="Пароль:" HorizontalAlignment="Left" Margin="17,60,0,0" VerticalAlignment="Top" FontSize="14"/>
        <TextBox x:Name="LoginText" HorizontalAlignment="Left" Height="23" Margin="80,24,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="163" FontSize="14" UndoLimit="20"/>
        <PasswordBox x:Name="PasswordText" HorizontalAlignment="Left" Margin="80,65,0,0" VerticalAlignment="Top" FontSize="14" Width="163"/>

    </Grid>
</Window>
